        }

        h3 {
            color: white;
        }

        h2 {
            color: white
        }

        h3 {
            color: white
        }

        @keyframes loading {
            0% { width: 0%; }
            100% { width: 100%; }
        }

        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        .loading-bar {
            position: fixed;
            bottom: 0;
            left: 0;
            height: 2px;
            background: linear-gradient(to right, #4f46e5, #818cf8);
            animation: loading 0.1s ease-in-out;
            z-index: 1000;
        }

        .spinner {
            position: fixed;
            bottom: 20px;
            right: 20px;
            animation: spin 0.1s linear infinite;
            z-index: 1000;
        }

        .font-mono {
            font-family: 'JetBrains Mono' !important;
        }
    </style>
